<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="BrowserStack Android" thread-count="2" parallel="tests">

    <test name="BS_Android_pixel3 Test">
        <parameter name="config" value="conf/browserstack/BS_Android.conf.json"/>
        <parameter name="environment" value="BS_Android_pixel3"/>
        <classes>
            <class name="automation.tests.ExperiBankTests"/>
        </classes>
    </test>

    <test name="BS_Android_SamsungGalaxyS10e Test">
        <parameter name="config" value="conf/browserstack/BS_Android.conf.json"/>
        <parameter name="environment" value="BS_Android_SamsungGalaxyS10e"/>
        <classes>
            <class name="automation.tests.ExperiBankTests"/>
        </classes>
    </test>

</suite>
